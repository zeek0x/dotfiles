[user]
	email = 0h23k1.n40k1@gmail.com
	name = zeek0x
[core]
	editor = vim
[pager]
	branch = cat
	status = cat
	diff = less
	log = less
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
[alias]
	a = add
	ap = add -p
	b = branch
	c = commit
	cm = commit -m
	camd = commit --amend
	d = diff
	s = status
	co = checkout
	cob = checkout -b
	lg = log --all --graph --oneline --decorate
	poh = push origin HEAD
	unchanged-on = update-index --assume-unchanged
	unchanged-off = update-index --no-assume-unchanged
[ghq]
	root = ~/src
[include]
	path = ~/.gitconfig.local
