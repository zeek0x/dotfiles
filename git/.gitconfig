[user]
	email = 0h23k1.n40k1@gmail.com
	name = OHZEKI Naoki
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[pager]
	branch = cat
	status = cat
	diff = less
	log = less
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
[alias]
	a = add
	ap = add -p
	b = branch
	c = commit
	cm = commit -m
	camd = commit --amend
	d = diff
	s = status
	co = checkout
	cob = checkout -b
	lg = log --graph --oneline --decorate
	poh = push origin HEAD
[fetch]
    prune = true
[pull]
    ff = only
[includeIf "gitdir:~/ghq/github.com/'Orgnaization'/**"]
    path = .gitconfig.local
